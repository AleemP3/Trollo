<%= form_with(model: [@board, @list], local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, required: true, placeholder: "Title" %>
  </div>
  <div class="field">
    <%= f.label :priority %>
    <%= f.text_field :priority, required: true, placeholder: "priority" %>
  </div>
  <%= f.submit class: "ui green button" %>
  <% end %> 